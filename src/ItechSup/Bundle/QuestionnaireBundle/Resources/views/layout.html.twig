{% endblock %}
{% block body %}
    <div class="row">
        <div class="col-lg-12">
            {{ app.user.username }} |
            <a href="{{ path('fos_user_security_logout') }}">
                {{ 'layout.logout'|trans({}, 'FOSUserBundle') }}
            </a>
        </div>

        <nav class="col-lg-2">
            <ul>
                <li><a href="{{ path('itechsup_questionnaire_userquestionnaire_index') }}" >Liste des questionnaire</a></li>
{#                <li><a href="{{ path('itech_sup_gestion') }}" >Gestion des qustionnaire</a></li>#}
            </ul>
        </nav>
        <div class="col-lg-10">
        {% block content %}{% endblock %}
    </div>
</div>
{% endblock %}
{% block javascripts %}
    <script src="{{ asset('bundles/itechsupquestionnaire/js/jquery.2.1.4.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/itechsupquestionnaire/js/bootstrap.min.js') }}" type="text/javascript"></script>
{% endblock %}
